
    <div class="container">
        <table class="table table-dark">
            <thead>
                <tr>
                    <th scope="col">ID</th>
                    <th scope="col">Name</th>
                    <th scope="col">Description</th>
                    <th scope="col">Price</th>
                    <th scope="col">Image</th>
                </tr>
            </thead>
            <tbody>
                <% products.forEach(element =>{ %>
                <tr>
                    <th><%= element.id  %></th>
                    <td><%= element.name %></td>
                    <td><%= element.desc %> </td>
                    <td scope="col"><%= element.price  %></td>
                    <td>
                        <img src="<%= element.image %>" />
                    </td>
                </tr>
                <% }) %>
                
            </tbody>
        </table>
    </div>
</body>
